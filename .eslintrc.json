{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-param-reassign": "off",
    "prettier/prettier": "error",
    "no-console": "off",
    "no-plusplus": "off"
  }
}
